[user]
    email = imvicencio@live.cl
    name = Ismael Vicencio

[color]
    diff = auto
    status = auto
    ui = auto

[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red
    new = green

[alias]
    pm = "!git push -u origin master"
    c = commit -m
    co = checkout
    a = add -A
    st = status
    pu = push
    pl = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit

[push]
    default = simple
